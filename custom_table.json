HTMLList = 
"<div class='sidepane-section'>
    <h2 class='heading' style='margin-bottom:1.6rem;color:#1C1C1C;'>Upcoming</h2>
    <ul class='list-unstyled'>" & 
    CONCATENATEX(
        'Table',
        "<li class='border-bottom padding-bottom-3x margin-bottom-3x list-item'>
            <div class='event-layout'>
                <div class='description'>
                    <div class='date h-8--sm padding-bottom-1x'>" & 'Table'[Date] & "</div>
                    <div class='title'>" & 'Table'[Event Title] & "</div>
                </div>
            </div>
        </li>",
        ""
    ) & 
    "</ul>
</div>"
